<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aicp"
           fetch="https://github.com/"
           review="gerrit.aicp-rom.com"
           revision="refs/heads/p9.0" />

  <!-- Remove unused LineageOS projects -->
  <remove-project name="LineageOS/android" />
  <remove-project name="LineageOS/android_build" />
  <remove-project name="LineageOS/android_frameworks_base" />
  <remove-project name="LineageOS/android_lineage-sdk" />
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <remove-project name="LineageOS/android_packages_apps_SetupWizard" />
  <remove-project name="LineageOS/android_vendor_lineage" />

  <remove-project name="LineageOS/ansible" />
  <remove-project name="LineageOS/charter" />
  <remove-project name="LineageOS/cm_crowdin" />
  <remove-project name="LineageOS/cve_tracker" />
  <remove-project name="LineageOS/hudson" />
  <remove-project name="LineageOS/mirror" />
  <remove-project name="LineageOS/slackbot" />
  <remove-project name="LineageOS/www" />
  <remove-project name="LineageOS/lineage_wiki" />
  <remove-project name="LineageOS/contributors-cloud-generator" />
  <remove-project name="LineageOS/scripts" />


  <!-- AICP repositories -->
  <project path="platform_manifest" name="AICP/platform_manifest" remote="aicp" />
  <project path="build/make" name="AICP/build" remote="aicp" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="frameworks/base" name="AICP/frameworks_base" remote="aicp" groups="pdk-cw-fs,pdk-fs" />
  <project path="frameworks/opt/aicpgear" name="AICP/frameworks_opt_aicpgear" remote="aicp" />
  <project path="lineage-sdk" name="AICP/lineage-sdk" remote="aicp" />
  <project path="packages/apps/AicpExtras" name="AICP/packages_apps_AicpExtras" remote="aicp" />
  <project path="packages/apps/Settings" name="AICP/packages_apps_Settings" remote="aicp" groups="pdk-fs" />
  <project path="packages/apps/AICP_OTA" name="AICP/packages_apps_AICP_OTA" remote="aicp" />
  <project path="vendor/aicp" name="AICP/vendor_aicp" remote="aicp" />

</manifest>
